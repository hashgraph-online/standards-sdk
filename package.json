    "buffer": "^6.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0",
    "file-type": "^20.5.0",
    "ioredis": "^5.8.2",
    "mime-types": "^2.1.35",
    "viem": "^2.19.9",
    "x402-axios": "^0.7.0",
    "zod": "^3.25.76"
  },
  "resolutions": {
    "@hashgraph/sdk": "2.75.0"
  },
